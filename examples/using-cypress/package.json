{
  "name": "using-cypress",
  "private": true,
  "description": "@freiksenet/gatsby example site that shows how to write accessibility tests with Cypress and axe.",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "@freiksenet/gatsby build",
    "develop": "@freiksenet/gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "@freiksenet/gatsby serve",
    "clean": "@freiksenet/gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "dependencies": {
    "@freiksenet/gatsby": "^2.21.22",
    "@freiksenet/gatsby-image": "^2.4.3",
    "@freiksenet/gatsby-plugin-manifest": "^2.4.2",
    "@freiksenet/gatsby-plugin-offline": "^3.2.1",
    "@freiksenet/gatsby-plugin-react-helmet": "^3.3.1",
    "@freiksenet/gatsby-plugin-sharp": "^2.6.2",
    "@freiksenet/gatsby-source-filesystem": "^2.3.1",
    "@freiksenet/gatsby-transformer-sharp": "^2.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.3.1",
    "axe-core": "^3.5.3",
    "cypress": "^4.5.0",
    "cypress-axe": "^0.8.1",
    "prettier": "2.0.5",
    "start-server-and-test": "^1.11.0"
  },
  "keywords": [
    "@freiksenet/gatsby"
  ]
}
